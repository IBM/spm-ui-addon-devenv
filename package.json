    "open": "^8.4.0",
    "prop-types": "*",
    "react": "^18.3.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^18.3.1",
    "react-intl": "^6.6.8",
    "regenerator-runtime": "^0.13.7",
